{
  "schemaVersion": 1,
  "id": "teleportation_potions",
  "version": "${version}",

  "name": "Teleportation Potions",
  "description": "A mod that adds recall potions inspired by Terraria to Minecraft",
  "authors": [
    "Hyper_Pigeon"
  ],
  "contact": {
    "homepage": "https://github.com/HyperPigeon",
    "sources": "https://github.com/HyperPigeon/TeleportationPotion"
  },

  "license": "CC0-1.0",
  "icon": "assets/teleportation_potions/textures/item/recall_potion.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.hyper_pigeon.teleportation_potions.TeleportationPotions"
    ]
  },
  "mixins": [
    "teleportation_potions.mixins.json"
  ],

  "depends": {
